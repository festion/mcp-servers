# Development Environment Configuration

# Deployment Settings
DEPLOY_ENV=dev
DEPLOY_HOST=localhost
DEPLOY_PORT=22
DEPLOY_USER=dev
DEPLOY_PATH=/home/dev/workspace/github-actions-runner
DEPLOY_INFRASTRUCTURE=true

# GitHub Configuration
GITHUB_TOKEN=${GITHUB_TOKEN}
GITHUB_ORG=your-org
GITHUB_REPO=your-repo
GITHUB_RUNNER_VERSION=2.311.0
GITHUB_RUNNER_COUNT=2
GITHUB_RUNNER_PORT=8080
GITHUB_RUNNER_LABELS=dev,self-hosted,linux,x64

# Service Configuration
MONITORING_PORT=9090
METRICS_PORT=8081
ENABLE_DEBUG=true
LOG_LEVEL=debug

# Security Settings
ENABLE_SSL=false
SSL_CERT_PATH=
SSL_KEY_PATH=
JWT_SECRET=dev-jwt-secret-change-in-production

# Storage Configuration
BACKUP_DIR=/tmp/deployment-backups
DATA_DIR=/var/lib/github-runner
LOG_DIR=/var/log/github-runner

# Database Configuration (if needed)
DATABASE_URL=sqlite:///tmp/github-runner-dev.db
REDIS_URL=redis://localhost:6379/0

# Resource Limits
CPU_LIMIT=2
MEMORY_LIMIT=4G
DISK_LIMIT=20G

# Monitoring Configuration
ENABLE_MONITORING=true
METRICS_RETENTION=7d
ENABLE_ALERTS=false

# Notification Settings
SLACK_WEBHOOK=
EMAIL_NOTIFICATIONS=false
NOTIFICATION_LEVEL=info

# Development Specific
ENABLE_HOT_RELOAD=true
ENABLE_PROFILING=true
ENABLE_TRACING=false